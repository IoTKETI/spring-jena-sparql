# baseURI: http://np.org/np
# imports: http://purl.uniprot.org/core/
# imports: http://www.w3.org/2002/07/owl#
# imports: http://www.w3.org/2004/02/skos/core

@prefix :        <http://np.org/np#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix up:      <http://purl.uniprot.org/core/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<http://np.org/np>
      rdf:type owl:Ontology ;
      owl:imports owl: , <http://www.w3.org/2004/02/skos/core> ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string .


:Entry
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      owl:equivalentClass up:Protein;
      .

:Isoform
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      owl:equivalentClass up:Protein.
      

:Term
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing;
      .

:Tissue
      rdf:type owl:Class ;
      rdfs:subClassOf :Term;
      .

:Evidence
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing;
      .
                
:Annotation
      rdf:type owl:Class ;
      rdfs:comment "General annotation of a protein."^^xsd:string ;
      rdfs:label "general annotation "^^xsd:string ;
      rdfs:seeAlso "http://www.nextprot.org/manual/Annotation"^^xsd:anyURI ;
      rdfs:subClassOf owl:Thing;
      .
      
:Feature
      rdf:type owl:Class ;
      rdfs:comment "General sequence annotation of a protein."^^xsd:string ;
      rdfs:label "general sequence annotation "^^xsd:string ;
      rdfs:seeAlso "http://www.nextprot.org/manual/Feature"^^xsd:anyURI ;
      rdfs:subClassOf :Annotation;
      .
            
#
#
# define global properties
:annotation
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Isoform ;
      rdfs:range :Annotation.

:in
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Annotation ;
      rdfs:range :Term.

:withExperimentalDesciption
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Annotation ;
      rdfs:range :Evidence.

#
#
# define expresion schema
:Expression
      rdf:type owl:Class ;
      rdfs:comment "General description of the expression of a protein."^^xsd:string ;
      rdfs:label "Expression"^^xsd:string ;
      rdfs:seeAlso "http://www.proteinatlas.org/"^^xsd:anyURI ;
      rdfs:seeAlso "http://bgee.unil.ch/bgee/bgee"^^xsd:anyURI ;
      rdfs:subClassOf :Annotation.


:expressed
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Isoform ;
      rdfs:subPropertyOf :annotation;
      rdfs:range :Expression.
      
:highlyExpressed
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Isoform ;
      rdfs:subPropertyOf :annotation;
      rdfs:range :Expression.
      
:notHighlyExpressed
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Isoform ;
      rdfs:subPropertyOf :annotation;
      rdfs:range :Expression.
      
:notExpressed
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Isoform ;
      rdfs:subPropertyOf :annotation;
      rdfs:range :Expression.
