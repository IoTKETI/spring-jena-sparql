#
# Define annotations relations.
#
 

:annotation
      rdf:type owl:ObjectProperty, owl:SymmetricProperty ;
      rdfs:domain :Isoform ;
      rdfs:range :Annotation.

#TODO precise which Ontology is available for this relation and list other relations depending 
#     the ontology
:in
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Annotation ;
      rdfs:range :Term.

      
      
:processing
   a rdf:Property, owl:FunctionalProperty;
   rdfs:subPropertyOf :annotation;
   rdfs:label "molecule processing"^^xsd:string; 
   rdfs:domain :Isoform ;
   rdfs:range  [ a owl:Class;
     owl:oneOf (:MoleculeProcessing :MatureProtein :MaturationPeptide :InitiatorMethionine :SignalPeptide :TransitPeptide );
   ];
   .

:regions
   a rdf:Property, owl:FunctionalProperty;
   rdfs:subPropertyOf :annotation;
   rdfs:label "region of interest"^^xsd:string; 
   rdfs:domain :Isoform ;
   rdfs:range  [ a owl:Class;
     owl:oneOf (:RegionOfInterest :CalciumBindingRegion :CoiledCoilRegion :CompositionallyBiasedRegion :DnaBindingRegion :Domain :InteractingRegion :ShortSequenceMotif :NucleotidePhosphateBindingRegion :Repeat :ZincFingerRegion :CodingSequence );
   ];
   .

:site
   a rdf:Property, owl:FunctionalProperty;
   rdfs:subPropertyOf :annotation;
   rdfs:label "site"^^xsd:string; 
   rdfs:domain :Isoform ;
   rdfs:range  [ a owl:Class;
     owl:oneOf (:Site :ActiveSite :BindingSite :CleavageSite :MetalIonBindingSite );
   ];
   .

:modifiedResidue
   a rdf:Property, owl:FunctionalProperty;
   rdfs:subPropertyOf :annotation;
