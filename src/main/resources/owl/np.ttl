:Evoc
  rdf:type owl:Class ;
  rdfs:subClassOf :Term;
  rdfs:label "eVOC"^^xsd:string;
  rdfs:comment "Expressed Sequence Annotation for Humans Provides structured controlled vocabularies for the annotation of expressed sequences with respect to anatomical system, cell type, developmental stage, experimental technique, microarray platform, pathology, pooling, tissue preparation and treatment."^^xsd:string;
  rdfs:seeAlso "http://www.nextprot.org/manual/Evoc"^^xsd:anyURI;
  .      
  
:EvocAnatomicalSystem
  rdf:type owl:Class ;
  rdfs:subClassOf :Term;
  rdfs:label "eVOC Anatomical System"^^xsd:string;
  rdfs:comment "eVOC ontology for anatomical system. http://trac.genebio.com/~defaultuser/repository/anatomicalsystem.obo"^^xsd:string;
  rdfs:seeAlso "http://www.nextprot.org/manual/EvocAnatomicalSystem"^^xsd:anyURI;
  .      
  
:EvocCellType
  rdf:type owl:Class ;
  rdfs:subClassOf :Term;
  rdfs:label "eVOC Cell Type"^^xsd:string;
