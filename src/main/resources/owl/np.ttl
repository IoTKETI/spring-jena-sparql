  rdfs:seeAlso "http://www.nextprot.org/manual/EvocCellType"^^xsd:anyURI;
  .      
  
:EvocDevelopmentStage
  rdf:type owl:Class ;
  rdfs:subClassOf :Term;
  rdfs:label "eVOC Development Stage"^^xsd:string;
  rdfs:comment "eVOC ontology for development stages. http://trac.genebio.com/~defaultuser/repository/developmentstage.obo"^^xsd:string;
  rdfs:seeAlso "http://www.nextprot.org/manual/EvocDevelopmentStage"^^xsd:anyURI;
