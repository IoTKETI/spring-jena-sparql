   rdfs:range  [ a owl:Class;
     owl:oneOf (:AminoAcidModification :CrossLink :DisulfideBond :GlycosylationSite :LipidMoietyBindingRegion :NonStandardAminoAcid );
   ];
   .

:structure
   a rdf:Property, owl:FunctionalProperty;
   rdfs:subPropertyOf :annotation;
   rdfs:label "secondary structure"^^xsd:string; 
   rdfs:domain :Isoform ;
   rdfs:range  [ a owl:Class;
     owl:oneOf (:SecondaryStructure :BetaStrand :Helix :Turn );
   ];
   .

:variant
   a rdf:Property, owl:FunctionalProperty;
   rdfs:subPropertyOf :annotation;
   rdfs:label "variant"^^xsd:string; 
   rdfs:domain :Isoform ;
   rdfs:range  [ a owl:Class;
     owl:oneOf (:Variant :AlignmentConflict :SequenceConflict :SequenceVariant );
   ];
   .

:misdefinedRegion
   a rdf:Property, owl:FunctionalProperty;
   rdfs:subPropertyOf :annotation;
   rdfs:label "misdefined region"^^xsd:string; 
   rdfs:domain :Isoform ;
   rdfs:range  [ a owl:Class;
     owl:oneOf (:MisdefinedRegion :NonConsecutiveResidues :NonTerminalResidue :UnsureResidue );
