<http://np.org/rdf>
      rdf:type owl:Ontology ;
      owl:imports owl: , <http://www.w3.org/2004/02/skos/core> ;
      owl:versionInfo "Initial release"^^xsd:string .


:Entry
      rdf:type owl:Class ;
      rdfs:comment "General information of a protein."^^xsd:string ;
      rdfs:subClassOf owl:Thing ;
      owl:equivalentClass up:Protein;
      .

:Isoform
      rdf:type owl:Class ;
      rdfs:comment "Specific information of a protein isoform."^^xsd:string ;
      rdfs:subClassOf owl:Thing ;
      .


#TODO generate specific owl for the publication hierarchy       
:Publication
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      .
      #
# Define Ontology
# 
:Term
      rdf:type owl:Class ;
      rdfs:comment "A concept used to classify resources."^^xsd:string ;
      rdfs:subClassOf owl:Thing;
      owl:equivalentClass up:Concept;
      .
  
:EnzymeClassification
  rdf:type owl:Class ;
  rdfs:subClassOf :Term;
  rdfs:label "Enzyme classification"^^xsd:string;
  rdfs:comment "Enzyme classification and enzyme description."^^xsd:string;
  rdfs:seeAlso "http://www.nextprot.org/manual/EnzymeClassification"^^xsd:anyURI;
  .      
  
:EvidenceCodeOntology
  rdf:type owl:Class ;
