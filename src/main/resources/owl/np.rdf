# baseURI: http://np.org/np
# imports: http://purl.uniprot.org/core/
# imports: http://www.w3.org/2002/07/owl#
# imports: http://www.w3.org/2004/02/skos/core

@prefix :        <http://np.org/np#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix up:      <http://purl.uniprot.org/core/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<http://np.org/np>
      rdf:type owl:Ontology ;
      owl:imports owl: , up: , <http://www.w3.org/2004/02/skos/core> ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string .


:Entry
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      owl:equivalentClass up:Protein .

:Isoform
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      owl:equivalentClass up:Protein .
      
      
:Go_Function
      rdf:type owl:Class ;
      rdfs:comment "General description of the functions of a protein."^^xsd:string ;
      rdfs:label "Function"^^xsd:string ;
      rdfs:seeAlso "http://www.uniprot.org/manual/function"^^xsd:anyURI ;
      rdfs:subClassOf up:Annotation ;
      owl:disjointWith up:Function_Annotation .

:Expression
      rdf:type owl:Class ;
      rdfs:comment "General description of the expression of a protein."^^xsd:string ;
      rdfs:label "Expression"^^xsd:string ;
      rdfs:seeAlso "http://www.proteinatlas.org/"^^xsd:anyURI ;
      rdfs:seeAlso "http://bgee.unil.ch/bgee/bgee"^^xsd:anyURI ;
      rdfs:subClassOf up:Annotation ;


:expressed
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Isoform ;
      rdfs:range :Expression;
      rdfs:subPropertyOf up:annotation .

:highlyExpressed
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Isoform ;
      rdfs:range :Expression;
      rdfs:subPropertyOf up:annotation .

:notHighlyExpressed
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Isoform ;
      rdfs:range :Expression;
      rdfs:subPropertyOf up:annotation .

:notExpressed
      rdf:type owl:ObjectProperty ;
      rdfs:domain :Isoform ;
      rdfs:range :Expression;
      rdfs:subPropertyOf up:annotation .

