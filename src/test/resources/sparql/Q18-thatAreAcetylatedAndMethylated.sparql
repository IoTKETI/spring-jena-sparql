#Q18 that are acetylated and methylated and located in the nucleus 
PREFIX : <http://nextprot.org/rdf#>
PREFIX term: <http://nextprot.org/rdf/terminology/>
SELECT distinct ?entry WHERE {
# in nucleus
  ?entry :isoform/:localisation/:in term:SL-0191.
# acetylated and methylated  
  ?entry :classifiedWith term:KW-0007.
  ?entry :classifiedWith term:KW-0488.
}